<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.text.Font?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<fx:root type="javafx.scene.layout.GridPane"
         xmlns:fx="http://javafx.com/fxml"
         alignment="center"
         prefHeight="450.0" prefWidth="350.0" hgap="10" vgap="50">
    <padding><Insets top="25" right="25" bottom="25" left="25"/></padding>
        <Label fx:id="loginLabel" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="1">
            <font>
                <Font size="14.0" />
            </font>
        </Label>

        <JFXTextField fx:id="username" labelFloat="true" prefHeight="33.0" prefWidth="250.0" minWidth="250.0" maxWidth="250.0" promptText="Username"
                      GridPane.columnIndex="0" GridPane.rowIndex="1">
            <font>
                <Font size="16.0" />
            </font>
        </JFXTextField>
        <JFXPasswordField fx:id="password" labelFloat="true" prefHeight="33.0" prefWidth="250.0" minWidth="250.0" maxWidth="250.0" promptText="Password"
                          GridPane.columnIndex="0" GridPane.rowIndex="2">
            <font>
                <Font size="17.0" />
            </font>
        </JFXPasswordField>

        <JFXButton fx:id="loginButton" layoutX="106.0" layoutY="278.0" prefHeight="40.0" prefWidth="250.0" minWidth="250.0" maxWidth="250.0" text="Login"
                   GridPane.columnIndex="0" GridPane.rowIndex="3"/>


</fx:root>